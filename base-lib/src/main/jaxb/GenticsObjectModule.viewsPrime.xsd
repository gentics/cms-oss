	<!-- type for a button component -->
	<xs:complexType name="ButtonComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="label" type="xs:string" minOccurs="1" maxOccurs="1"/>
