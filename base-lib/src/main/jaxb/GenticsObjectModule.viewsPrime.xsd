	<!-- type for an DatasourceListComponent -->
	<xs:complexType name="DatasourceListComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:group ref="DatasourceListComponentSettings">
			<xs:annotation>
				<xs:appinfo>
					<jaxb:property name="settings"/>
				</xs:appinfo>
			</xs:annotation>
		</xs:group>
		<xs:attributeGroup ref="ComponentAttributes"/>
	</xs:complexType>

	<xs:group name="DatasourceListComponentSettings">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class name="JAXBDSListCompSettings"/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="columns" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="column" type="Column" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="sortby" type="xs:token" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="sortBy"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="sortorder" type="sortordertype" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="sortOrder"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="sortable" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="filterrule" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="filterRule"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="rule" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="help" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="filterlabel" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="filterLabel"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="paging" type="ComponentPaging" minOccurs="0" maxOccurs="1"/>
