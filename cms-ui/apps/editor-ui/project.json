{
    "name": "editor-ui",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "prefix": "gtx",
    "sourceRoot": "apps/editor-ui/src",
    "tags": ["app", "ui", "angular"],
    "targets": {
        "build": {
            "executor": "@nx/angular:webpack-browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "customWebpackConfig": {
                    "path": "apps/editor-ui/webpack.partial.js"
                },
                "outputPath": "dist/apps/editor-ui",
                "index": "apps/editor-ui/src/index.html",
                "main": "apps/editor-ui/src/main.ts",
                "polyfills": ["apps/editor-ui/src/polyfills.ts"],
                "tsConfig": "apps/editor-ui/tsconfig.app.json",
                "assets": [
                    {
                        "glob": "**/*",
                        "input": "apps/editor-ui/src/assets",
                        "output": "assets"
                    }
                ],
                "styles": ["apps/editor-ui/src/styles/styles.scss"],
                "stylePreprocessorOptions": {
                    "includePaths": [
                        "apps/editor-ui/src/styles",
                        "libs",
                        "node_modules"
                    ]
                },
                "scripts": [],
                "allowedCommonJsDependencies": [
                    "@bevacqua/rome",
                    "@gentics/ui-core",
                    "angular2-hotkeys",
                    "dom-serialize",
                    "masonry-layout",
                    "rxjs",
                    "serialize-to-js",
                    "immutablets"
                ],
                "fileReplacements": [
                    {
                        "replace": "apps/editor-ui/src/environments/environment.ts",
                        "with": "apps/editor-ui/src/environments/environment.prod.ts"
                    }
                ],
                "outputHashing": "all",
                "optimization": true,
                "sourceMap": false,
                "namedChunks": false,
                "extractLicenses": true,
                "vendorChunk": false,
                "buildOptimizer": true,
                "aot": true
            },
            "configurations": {
                "production": {},
                "development": {
                    "fileReplacements": [],
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                },
                "ci": {
                    "progress": false
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@nx/angular:webpack-dev-server",
            "options": {
                "proxyConfig": "proxy.conf.json",
                "liveReload": false
            },
            "configurations": {
                "production": {
                    "buildTarget": "editor-ui:build:production"
                },
                "development": {
                    "buildTarget": "editor-ui:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "apps/editor-ui/**/*.ts",
                    "apps/editor-ui/**/*.html"
                ]
            }
        },
        "serve-static": {
            "executor": "@nx/web:file-server",
            "options": {
                "buildTarget": "editor-ui:build"
            },
            "configurations": {
                "e2e": {
                    "proxyUrl": "http://localhost:8080",
                    "proxyOptions": {
                        "secure": false,
                        "changeOrigin": true
                    }
                }
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "cwd": ".",
                "command": "node tools/ng-test-runner editor-ui --watch=false --forceAngular"
            },
            "configurations": {
                "watch": {
                    "command": "node tools/ng-test-runner editor-ui --watch=true --forceAngular"
                },
                "ci": {
                    "command": "node tools/ng-test-runner editor-ui --watch=false --forceAngular --configuration=ci",
                    "color": false
                }
            }
        },
        "component-test": {
            "executor": "@nx/cypress:cypress",
            "options": {
                "cypressConfig": "apps/editor-ui/cypress.config.ts",
                "testingType": "component",
                "skipServe": true,
                "devServerTarget": "editor-ui:build",
                "watch": false
            },
            "configurations": {
                "watch": {
                    "watch": true
                },
                "ci": {
                    "quiet": true,
                    "devServerTarget": "editor-ui:build:ci",
                    "reporterOptions": {
                        "color": false,
                        "reporter": "min"
                    }
                }
            }
        }
    }
}
