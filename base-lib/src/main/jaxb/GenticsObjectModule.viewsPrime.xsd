			</xs:element>
			<xs:element name="link" type="Link" minOccurs="0" maxOccurs="1">
				<!-- @deprecated replaced by actions -->
			</xs:element>
			<xs:element name="links" type="LinkList" minOccurs="0" maxOccurs="1">
				<!-- @deprecated replaced by actions -->
			</xs:element>
			<xs:element name="actions" type="Actions" minOccurs="0" maxOccurs="1">
