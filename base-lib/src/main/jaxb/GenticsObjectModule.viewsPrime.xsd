<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="1.0">
	<xs:annotation>
		<xs:appinfo>
			<jaxb:globalBindings generateIsSetMethod="true">
				<jaxb:javaType name="java.lang.Integer" xmlType="xs:integer" parseMethod="valueOf" printMethod="toString"/>
			</jaxb:globalBindings>
		</xs:appinfo>
	</xs:annotation>
	<!-- main node (views) -->
	<xs:element name="views">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="view" type="view" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="defaultview" type="xs:token" use="optional">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="defaultView"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!-- type for a specific view -->
	<xs:complexType name="view">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="viewNodes" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="settings"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:group>
			<xs:group ref="AnyComponent" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="children"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:group>
			<xs:element name="defaultbuttons" minOccurs="0" maxOccurs="1">
				<!-- @deprecated buttons shall all be defined with buttoncomponents -->
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="defaultButtonContainer"/>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="button" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:appinfo>
									<jaxb:property name="buttons"/>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:all>
									<xs:element name="label" type="xs:string" minOccurs="0" maxOccurs="1"/>
									<xs:element name="enabled" type="PBoolean" minOccurs="0" maxOccurs="1"/>
									<xs:element name="optional" type="PBoolean" minOccurs="0" maxOccurs="1"/>
									<xs:element name="visible" type="PBoolean" minOccurs="0" maxOccurs="1"/>
									<xs:element name="actions" type="Actions" minOccurs="0" maxOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<jaxb:property name="actionContainer"/>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:all>
								<xs:attribute use="required" type="defaultbuttonid" name="id"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="class" type="xs:token" use="optional">
			<xs:annotation>
				<xs:appinfo>
					<jaxb:property name="viewClass"/>
				</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="id" type="xs:token" use="optional"/>
		<xs:attribute name="label" type="xs:string" use="optional"/>
		<xs:attribute name="doubleclickprotection" type="xs:boolean" use="optional"/>
	</xs:complexType>

	<!-- type of the id of a default button -->
	<xs:simpleType name="defaultbuttonid">
	  <!-- @deprecated editing shall be implemented using buttoncomponents and pluggable actions -->
	  <xs:restriction base="xs:token">
		<xs:enumeration value="ok"/>
		<xs:enumeration value="edit"/>
		<xs:enumeration value="delete"/>
		<xs:enumeration value="cancel"/>
		<xs:enumeration value="search"/>
	  </xs:restriction>
	</xs:simpleType>

	<!-- type for a list component -->
	<xs:complexType name="ListComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="content" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:group ref="AnyComponent">
						<xs:annotation>
							<xs:appinfo>
								<jaxb:property name="children"/>
							</xs:appinfo>
						</xs:annotation>
					</xs:group>
				</xs:complexType>
			</xs:element>
			<xs:element name="label" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="help" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="enabled" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="optional" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="visible" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="errortext" type="xs:string" minOccurs="0" maxOccurs="1">
				<!-- @deprecated undocumented and never used anywere -->
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="errorText"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="properties" type="ComponentPropertiesType" minOccurs="0" maxOccurs="1"/>
			<!-- TODO add all general component attributes -->
		</xs:all>
		<xs:attributeGroup ref="ComponentAttributes"/>
	</xs:complexType>
	<!-- type for a tab component -->
	<xs:complexType name="TabComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="content">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="tab" type="Tab" minOccurs="1" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="label" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="help" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="enabled" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="optional" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="visible" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="activetab" type="xs:token" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="activeTab"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="properties" type="ComponentPropertiesType" minOccurs="0" maxOccurs="1"/>
			<!-- TODO add all general component attributes -->
		</xs:all>
		<xs:attributeGroup ref="ComponentAttributes"/>
	</xs:complexType>
	<!-- type for a tab component -->
	<xs:complexType name="Tab">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="content">
				<xs:complexType>
					<xs:group ref="AnyComponent">
						<xs:annotation>
							<xs:appinfo>
								<jaxb:property name="children"/>
							</xs:appinfo>
						</xs:annotation>
					</xs:group>
				</xs:complexType>
			</xs:element>
			<xs:element name="enabled" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="optional" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="visible" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="label" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="properties" type="ComponentPropertiesType" minOccurs="0" maxOccurs="1"/>
		</xs:all>
		<xs:attribute use="required" name="id" type="xs:token"/>
	</xs:complexType>
	<!-- type for a text component -->
	<xs:complexType name="TextComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="label" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="help" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="syntax" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="syntax-description" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="length" type="xs:integer" minOccurs="0" maxOccurs="1">
				<!-- @deprecated add minlength and maxlength as addition to syntax -->
			</xs:element>
			<xs:element name="enabled" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="unique" type="PBoolean" minOccurs="0" maxOccurs="1">
				<!-- @deprecated must be implemented with pluggable action -->
			</xs:element>
			<xs:element name="optional" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="visible" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="properties" type="ComponentPropertiesType" minOccurs="0" maxOccurs="1"/>
		</xs:all>
		<xs:attributeGroup ref="ComponentAttributes"/>
	</xs:complexType>
	<!-- type for a numbercomponent -->
	<xs:complexType name="NumberComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="label" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="help" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="enabled" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="optional" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="visible" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="min" type="xs:double" minOccurs="0" maxOccurs="1"/>
			<xs:element name="max" type="xs:double" minOccurs="0" maxOccurs="1"/>
			<xs:element name="type" type="NumberComponentType" minOccurs="0" maxOccurs="1" default="double"/>
			<xs:element name="fractiondigits" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="fractionDigits"/>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:int">
							<xs:attribute name="failifmore" type="xs:boolean" use="optional" default="false">
								<xs:annotation>
									<xs:appinfo>
										<jaxb:property name="failIfMore"/>
									</xs:appinfo>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="properties" type="ComponentPropertiesType" minOccurs="0" maxOccurs="1"/>
		</xs:all>
		<xs:attributeGroup ref="ComponentAttributes"/>
	</xs:complexType>
	<!-- type for a information component -->
	<xs:complexType name="InformationComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="label" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="help" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="reference" type="xs:token" minOccurs="1" maxOccurs="1"/>
			<xs:element name="visible" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="optional" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="enabled" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="properties" type="ComponentPropertiesType" minOccurs="0" maxOccurs="1"/>
		</xs:all>
		<xs:attributeGroup ref="ComponentAttributes"/>
	</xs:complexType>
	<!-- type for a file upload component -->
	<xs:complexType name="FileUploadComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="label" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="help" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="enabled" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="optional" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="visible" type="PBoolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="contenttypeattribute" type="xs:token" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="contentTypeAttribute"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="filesizeattribute" type="xs:token" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="fileSizeAttribute"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="uploaddateattribute" type="xs:token" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="uploadDateAttribute"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="filenameattribute" type="xs:token" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="fileNameAttribute"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="keepcontent" type="xs:boolean" minOccurs="0" maxOccurs="1" default="true">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="keepContent"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="properties" type="ComponentPropertiesType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="downloadactions" type="Actions" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:property name="downloadActionContainer"/>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:all>
		<xs:attributeGroup ref="ComponentAttributes"/>
	</xs:complexType>
	<!-- type for a password component -->
	<xs:complexType name="PasswordComponent">
		<xs:annotation>
			<xs:appinfo>
				<jaxb:class/>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="label" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="labelverify" type="xs:string" minOccurs="0" maxOccurs="1">
