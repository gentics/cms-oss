{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "admin-ui": {
      "projectType": "application",
      "schematics": {
        "@nx/angular:component": {
          "style": "scss",
          "styleext": "scss"
        }
      },
      "root": "apps/admin-ui",
      "sourceRoot": "apps/admin-ui/src",
      "prefix": "gtx",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "apps/admin-ui/webpack.partial.js",
              "replaceDuplicatePlugins": true
            },
            "allowedCommonJsDependencies": [
              "@bevacqua/rome",
              "@gentics/ui-core",
              "angular2-hotkeys",
              "dom-serialize",
              "punycode",
              "semver",
              "serialize-to-js",
              "stacktrace-js"
            ],
            "outputPath": "dist/apps/admin-ui",
            "index": "apps/admin-ui/src/index.html",
            "main": "apps/admin-ui/src/main.ts",
            "polyfills": "apps/admin-ui/src/polyfills.ts",
            "tsConfig": "apps/admin-ui/tsconfig.app.json",
            "webWorkerTsConfig": "apps/admin-ui/tsconfig.worker.json",
            "assets": ["apps/admin-ui/src/assets"],
            "styles": ["apps/admin-ui/src/styles/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/admin-ui/src/styles",
                "libs",
                "node_modules"
              ]
            }
          },
          "defaultConfiguration": "development",
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/admin-ui/src/environments/environment.ts",
                  "with": "apps/admin-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb",
                  "maximumError": "20mb"
                }
              ]
            },
            "development": {
              "aot": true,
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "admin-ui:build",
            "proxyConfig": "proxy.conf.json",
            "liveReload": false
          },
          "defaultConfiguration": "development",
          "configurations": {
            "production": {
              "browserTarget": "admin-ui:build:production"
            },
            "development": {
              "browserTarget": "admin-ui:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/admin-ui/src/test.ts",
            "tsConfig": "apps/admin-ui/tsconfig.spec.json",
            "karmaConfig": "apps/admin-ui/karma.conf.js",
            "polyfills": "apps/admin-ui/src/polyfills.ts",
            "styles": ["apps/admin-ui/src/styles/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/admin-ui/src/styles",
                "libs",
                "node_modules"
              ]
            },
            "assets": ["apps/admin-ui/src/assets"],
            "webWorkerTsConfig": "apps/admin-ui/tsconfig.worker.json"
          }
        }
      }
    },
