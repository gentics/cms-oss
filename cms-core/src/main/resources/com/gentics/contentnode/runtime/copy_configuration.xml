				INNER JOIN template_folder AS tf2 ON tf2.folder_id = tf1.folder_id
				INNER JOIN template AS t2 ON t2.id = tf2.template_id
				WHERE t1.id = ? AND t1.name = t2.name
			</property>
			<property id="checkexcluded">true</property>
